# 克隆仓库到本地
git clone https://github.com/yangfanmenghe/yangfanmenghe.github.io.git
 
# 进入仓库目录
cd yangfanmenghe.github.io
 
# 创建index.html文件并编辑
echo "<html><body>Hello, GitHub Pages!</body></html>" > index.html
 
# 添加文件到仓库
git add .
 
# 提交更改
git commit -m "Initial deployment"
 
# 推送到GitHub
git push origin master
